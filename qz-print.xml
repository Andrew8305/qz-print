  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.qz-print:jar" value="${basedir}/out/artifacts/qz_print_jar"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.qz-print.jar" value="${artifacts.temp.dir}/qz-print.jar"/>
  </target>
  
  <target name="artifact.qz-print:jar" depends="init.artifacts, compile.module.qz-print, compile.module.jssc_2.6.0_qz, compile.module.pdf-renderer_0.9.1_qz" description="Build &#39;qz-print:jar&#39; artifact">
    <property name="artifact.temp.output.qz-print:jar" value="${artifacts.temp.dir}/qz_print_jar"/>
    <mkdir dir="${artifact.temp.output.qz-print:jar}"/>
    <jar destfile="${temp.jar.path.qz-print.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset dir="${qz-print.output.dir}"/>
      <zipfileset dir="${jssc_2.6.0_qz.output.dir}"/>
      <zipfileset dir="${pdf-renderer_0.9.1_qz.output.dir}"/>
      <zipfileset src="${basedir}/qz-print/lib/javaws.jar"/>
      <zipfileset src="${basedir}/qz-print/lib/pdf-renderer_qz.jar"/>
      <zipfileset src="${basedir}/qz-print/lib/plugin.jar"/>
      <zipfileset src="${basedir}/qz-print/lib/jssc_qz.jar"/>
    </jar>
    <copy file="${temp.jar.path.qz-print.jar}" tofile="${artifact.temp.output.qz-print:jar}/qz-print.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.qz-print:jar" description="Build all artifacts">
    <mkdir dir="${artifact.output.qz-print:jar}"/>
    <copy todir="${artifact.output.qz-print:jar}">
      <fileset dir="${artifact.temp.output.qz-print:jar}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
